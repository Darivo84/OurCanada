RewriteEngine On

RewriteRule ^robots.txt$ /robots.txt.php

RewriteRule ^contact-us/([A-Za-z0-9_-]+)$ /contact-us.php [QSA]
RewriteRule ^terms/([A-Za-z0-9_-]+)$ /terms.php [QSA]


RewriteRule ^community/([A-Za-z0-9_-]+)$ /cms_index.php [QSA]

RewriteRule ^([A-Za-z0-9_-]+)$ /index.php?url=$1 [QSA]

RewriteBase /

RewriteCond %{REQUEST_URI} ^/404/$
RewriteRule ^(.*)$ index.php [L]


ErrorDocument 404    /index.php?url=$1



