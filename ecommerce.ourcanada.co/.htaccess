ErrorDocument 404 https://%{HTTP_HOST}/404/
ErrorDocument 404    /index.php?url=$1
RewriteBase /

RewriteCond %{REQUEST_URI} ^/404/$
RewriteRule ^(.*)$ index.php [L]

RewriteEngine On
RewriteBase /
RewriteRule ^e-commerce/([0-9a-zA-Z_-]*)$ e-commerce.php [NC,L]
RewriteRule ^e-commerce/([0-9a-zA-Z_-]*)/([0-9a-zA-Z_-]*)$ e-commerce.php [NC,L]
RewriteRule ^e-commerce/([0-9a-zA-Z_-]*)/([0-9a-zA-Z_-]*)/([0-9a-zA-Z_-]*)$ e-commerce.php [NC,L]
RewriteCond %{REQUEST_URI} ^/404/$
RewriteRule ^(.*)$ 404.php [L]
RewriteEngine On
#RewriteCond %{SERVER_PORT} 80
#RewriteCond %{HTTPS} off
#RewriteRule ^(.*)$ https://%{HTTP_HOST}%{REQUEST_URI} [L,R=301]
RewriteRule ^product/([A-Za-z0-9_-]+)$ /product-detail.php [QSA]
RewriteRule ^category/([A-Za-z0-9_-]+)$ /category-list.php [QSA]

RewriteCond %{SCRIPT_FILENAME} !-d
RewriteRule ^([^.]+)$ $1.php [NC,L]
RewriteRule ^travel/([^/]+)/(.*)$ travel.php?control=$1&query=$2
RewriteRule ^travel/(.*)$ travel.php?control=$1&query=$2
RewriteRule ^travel/ travel.php?control=$1&query=$2
RewriteRule ^travel travel.php?control=$1&query=$2

RewriteCond %{SCRIPT_FILENAME} !-d
RewriteRule ^([^.]+)$ $1.php [NC,L]

